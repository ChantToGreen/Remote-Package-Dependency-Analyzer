<Window x:Class="Client.AnalysisResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gui"
        mc:Ignorable="d"
        Title="AnalysisResult" Height="500" Width="400">
    <Grid>
        <Border>
            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Margin="5,5,5,5">
                <TextBlock x:Name="resultView" FontFamily="consolas" FontSize="18" Padding="13"></TextBlock>
            </ScrollViewer>
        </Border>
        
    </Grid>
</Window>
